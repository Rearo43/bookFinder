<!DOCTYPE html>
<html lang="en"></html>
  <head>
    <%- include('../layout/head') %>
  </head>
  <nav>
    <div>Book App</div>
  </nav>
  <header>
    <div><%=count%> Saved Books</div>
  </header>
  <body>
    <% data.forEach(book => { %>
    <section>
      <div class="img">
        <img src=" <%= book.image_url %> " />
      </div>
      <h2><%= book.title %></h2>
      <h3><%= book.author %></h3>
      <button>View Details</button>
    </section>
    <% }) %>

  </body>
  <footer></footer>
</html>


